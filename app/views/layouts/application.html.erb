<!DOCTYPE html>
<html>
<head>
  <title>Shop</title>
  <%= csrf_meta_tags %>
  <%= csp_meta_tag %>

  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

  <%= stylesheet_pack_tag 'application' %>
</head>

<body>
  <div class="container">
    <p id="alert"><%= alert %></p>
    <p id="notice"><%= notice %></p>
    <%= yield %>
  </div>
</body>
</html>
